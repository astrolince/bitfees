{
    "version": 2,
    "builds": [
        {
            "src": "api/blocks/index.js",
            "use": "@now/node"
        },
        {
            "src": "api/blocks/update/index.js",
            "use": "@now/node"
        },
        {
            "src": "api/fees/index.js",
            "use": "@now/node"
        },
        {
            "src": "api/fees/update/index.js",
            "use": "@now/node"
        },
        {
            "src": "api/price/index.js",
            "use": "@now/node"
        },
        {
            "src": "api/price/update/index.js",
            "use": "@now/node"
        },
        {
            "src": "api/tweet/index.js",
            "use": "@now/node"
        },
        {
            "src": "api/tweet/update/index.js",
            "use": "@now/node"
        }
    ],
    "env": {
        "BITCOINAVERAGE_PUBLIC": "@bitcoinaverage_public",
        "BITCOINAVERAGE_SECRET": "@bitcoinaverage_secret",
        "REDIS_PORT": "@coiny_redis_port",
        "REDIS_HOST": "@coiny_redis_host",
        "REDIS_PASS": "@coiny_redis_pass",
        "TW_CONSUMER_KEY": "@coiny_tw_consumer_key",
        "TW_CONSUMER_SECRET": "@coiny_tw_consumer_secret",
        "TW_ACCESS_TOKEN_KEY": "@coiny_tw_access_token_key",
        "TW_ACCESS_TOKEN_SECRET": "@coiny_tw_access_token_secret",
        "MASTODON_ACCESS_TOKEN": "@coiny_mastodon_access_token"
    }
}